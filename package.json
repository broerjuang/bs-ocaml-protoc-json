{
  "name": "bs-bs-ocaml-protoc-json-json",
  "version": "0.0.0",
  "description": "Protobuf JSON runtime for BuckleScript",
  "keywords": ["protobuf", "serialization", "syntax", "bucklescript", "json"],
  "homepage": "https://github.com/mransan/bs-ocaml-protoc-json",
  "bugs": "https://github.com/mransan/bs-ocaml-protoc-json/issues",
  "license": "MIT",
  "author": " Maxime Ransan<maxime.ransan@gmail.com>",
  "repository" : { 
    "type": "git", 
    "url": "https://github.com/mransan/bs-ocaml-protoc-json"
  },
  "dependencies": {
    "bs-ppx_deriving_protobuf": "^2.5.x",
    "bs-bs-ocaml-protoc-json": "^1.0.x"
  },
  "peerDependencies" : {
    "bs-platform" : "^1.5.x"
  },
  "scripts": {
    "gen-example": "ocaml-protoc -json -ml_out tests tests/example.proto",
    "gen-unittest": "ocaml-protoc -json -ml_out tests tests/unittest.proto",
    "gen": "npm run-script gen-example && npm run-script gen-unittest",
    "build": "npm run-script gen && bsb -make-world",
    "test": "npm run-script build && node lib/js/tests/"
  }
}
